  <%= content_tag :table, class: "display compact table_intention", style:"width:100%", data: { url: intentions_path(format: :json)} do %>
  
    <thead>
    <tr id="header_table">
    <th><%= button_tag onclick:"checkbox_intention_datatable()", id:"btn_delete_selected" do %><i class="material-icons" id="delete_all" style="visibility:hidden">delete_outline</i><% end %>
      <div class="mdl-tooltip mdl-tooltip--large" for="delete_all"><span id="count-checked-checkboxes"></span> Selected(s)</div></th>
      <th class="mdl-data-table__cell--non-numeric" width="20%" id="head_table">Intention Code</th>
      <th class="mdl-data-table__cell--non-numeric" width="25%" id="head_table">Intention Name</th>
      <th class="mdl-data-table__cell--non-numeric" width="25%" id="head_table">Intention Tag</th>
      <th class="mdl-data-table__cell--non-numeric" width="20%">Last updated</th>
      <th width="10%">Action</th>
    </tr>
    </thead>
    <tbody>
    
    </tbody>
<% end %>

 <script>
$("#test").click(function(){
    var reg = $('#test2').val();
    $(".table_intention").DataTable().search(reg,true,true, true,true).draw();
});

  $(document).ready(function() {
  
    $("#btn_submit").click(function(){
    var $form = $('#delete_select').closest('form');
    $form.find('#btn_delete_selected').click();
    });
  });

</script>

